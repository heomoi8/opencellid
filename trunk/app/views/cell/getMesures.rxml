xml.instruct! :xml, :version=>"1.0",:encoding=>"UTF-8"
if @cell
 xml.cell (:lat=>@cell.lat,:lon=>@cell.lon,:mcc=>@cell.mcc,:mnc=>@cell.mnc,:lac=>@cell.lac,:cellId=>@cell.cellid,:nbSamples=>@cell.nbSamples) do
  @cell.mesures.each do|mesure|
    xml.mesure ("",:lat=>mesure.lat,:lon=>mesure.lon,:takenOn=>mesure.created_at,:takenBy=>mesure.userid)
  end
  
 end
else
  xml.error "Error, cell not found"
end
