xml.instruct! :xml, :version=>"1.0",:encoding=>"UTF-8"
if @cell
 xml.rsp(:stat=>"ok") do
    xml.cell (:lat=>@cell.lat,:lon=>@cell.lon,:mcc=>@cell.mcc,:mnc=>@cell.mnc,:lac=>@cell.lac,:cellId=>@cell.cellid,:nbSamples=>@cell.nbSamples) 
 end
else
 if @lac
 else
  xml.rsp(:stat=>"fail") {
    xml.err(:code=>1,:info=> " cell not found")
  }
end
end
